<div class="mt-6 prompts" id="selectedPrompts">
    {% for prompt in prompts %}
        <div id="prompts" class="p-4 bg-white border border-gray-200 rounded-lg mb-4" data-id="{{ prompt.id }}" data-title="{{ prompt.title }}" data-body="{{ prompt.body }}">
            <h3 class="text-lg font-semibold">{{ prompt.title }}</h3>
            <p class="text-gray-700 mt-2 prompt-body">{{ prompt.body }}</p>
        </div>
    {% endfor %}
</div>